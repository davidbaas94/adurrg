<%- include layouts/headeradmin.ejs %> 
	<!--start-breadcrumbs-->
	<div class="breadcrumbs">
		<div class="container">
			<div class="breadcrumbs-main">
				<ol class="breadcrumb">
					<li><a>Inicio</a></li>
					<li class="active">Registro de Galeria</li>
				</ol>
			</div>
		</div>
	</div>
	<!--end-breadcrumbs-->
	<!--start-account-->
	<div class="account">
		<div class="container"> 
			<div class="account-bottom">
				<div class="col-md-3 account-left">
				</div>
				<div class="col-md-6 account-left">
					<div class="alert alert-warning" role="alert">
						Selecciona la imagen correcta solo puedes subir la imagen una vez
					 </div>
					<form action="/galeria" method="POST" enctype="multipart/form-data">
					<div class="account-top heading">						 
						<h3>Registro de Imagenes</h3>
					</div>
					<div class="address">
						<span>Imagen</span>
						<input type="file" accept=".jpg, .jpge, .png" name="img" required>
					</div>
					<div class="address new">
						<input type="submit" value="Registrar">&nbsp;&nbsp;&nbsp;&nbsp;
						<a style="color: white" href="/listaproduc"  class="btn btn-warning">Cancelar</a>
					</div>
					</form>
				</div>									
			</div>
		</div>
	</div>
	<!--end-account-->
	<%- include layouts/footeradmin.ejs %>