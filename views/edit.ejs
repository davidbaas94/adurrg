<%- include layouts/headeradmin.ejs %> 
	<!--start-breadcrumbs-->
	<div class="breadcrumbs">
		<div class="container">
			<div class="breadcrumbs-main">
				<ol class="breadcrumb">
					<li><a>Inicio</a></li>
					<li class="active">Registro de manuales</li>
				</ol>
			</div>
		</div>
	</div>
	<!--end-breadcrumbs-->
	<!--start-account-->
	<div class="account">
		<div class="container"> 
			<div class="account-bottom">
				<div class="col-md-3 account-left">
				</div>
				<div class="col-md-6 account-left">										
					<% for(var manual=0; manual<Manuales.length; manual++){%>
					<form action="/edit/<%= Manuales[manual].Idmanual %>" method="POST">					
					<div class="account-top heading">						 
						<h3>Registro de Manuales</h3>
                    </div>					 					
					<div class="address">
						<span>Titulo</span> 
						<input type="text" name="titulo" value="<%=Manuales[manual].titulo %>" required>
					</div>
					<div class="address">
						<span>Descrioción</span>
						<input type="text" name="descripcion" value="<%=Manuales[manual].descripcion %>" required>  
					</div>
					<div class="address">
						<span>Precio</span>
						<input type="text" name="precio" value="<%=Manuales[manual].precio %>" required>
					</div>		
					<div class="address">
						<span>Autor</span>
						<input type="text" name="autor" value="<%=Manuales[manual].autor %>" required>
					</div>
					<div class="address">
						<span>Cantidad Disponible</span>
						<input type="text" name="cantidaddispo"  value="<%=Manuales[manual].cantidaddispo %>"  required>
					</div>
					<div class="address">
						<span>Técnologia</span>
						<select required name="tecnologia">							
							<option value="Node JS">Node JS</option>
						</select>
					</div>  
					<div class="address new">
						<input type="submit" value="Registrar">&nbsp;&nbsp;&nbsp;&nbsp;
						<a style="color: white" href="/listaproduc"  class="btn btn-warning">Cancelar</a>
					</div>					
					</form>
					<% } %> 											
				</div>									
			</div>
		</div>
	</div>
	<!--end-account-->
<%- include layouts/footeradmin.ejs %>